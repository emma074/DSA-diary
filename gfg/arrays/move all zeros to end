class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int c=0,index=0;
        int n=arr.length;
        for(int i=0;i<n;i++){
            if(arr[i]!=0){
                arr[index++]=arr[i];
            }
        }
        while(index<arr.length){
            arr[index++]=0;
    }
}
}